# -*- drop check constraint -*-
# name: dck
# key: dck
# expand-env: ((yas-indent-line 'fixed) (yas-wrap-around-region 'nil))
# --
if exists(SELECT top 1 null from information_schema.check_constraints where constraint_name='CH_$1_$2')
    alter table dbo.${1:/*Tab*/} drop constraint CH_$1_${2:Descr};$0