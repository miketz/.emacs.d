# -*- function *-*
# name: function
# key: fn
# expand-env: ((yas-indent-line 'fixed) (yas-wrap-around-region 'nil))
# --
if object_id('dbo.$1', 'FN') is null
    exec('create function dbo.$1() returns int as begin return 0 end');
GO

alter function dbo.${1:/*FuncName*/} (
    @${3:/*param1Name*/} ${4:/*type*/}
)
RETURNS ${2:/*ReturnType*/}
as
begin
    $0
    return /*val*/;
end
GO