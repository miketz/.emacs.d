# -*- stored proc -*-
# name: sp
# key: sp
# expand-env: ((yas-indent-line 'fixed) (yas-wrap-around-region 'nil))
# --
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

if object_id('$1', 'P') is not null
    drop procedure $1;
GO
create procedure dbo.${1:/*ProcName*/} (
    @${2:/*param1Name*/} ${3:/*type*/}
)
as

    $0
GO