# -*- stored proc -*-
# name: sp
# key: sp
# expand-env: ((yas-indent-line 'fixed) (yas-wrap-around-region 'nil))
# --
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

if object_id('dbo.$1', 'P') is null
    exec('create procedure dbo.$1 as select 0');
GO

alter procedure dbo.${1:/*ProcName*/} (
    @${2:/*param1Name*/} ${3:/*type*/}
)
as

    $0
GO